{
   "python.linting.pylintEnabled": false,
   "python.linting.enabled": true,
   "python.linting.pep8Enabled": true,
   "python.linting.flake8Enabled": true,
   "python.terminal.activateEnvironment": false,
   "python.formatting.autopep8Path": "/.venv/Lib/site-packages/autopep8-1.7.0.dist-info",
   "python.linting.flake8Path": "/.venv/Lib/site-packages/flake8",
   "cornflakes.linter.executablePath": "/.venv/Lib/site-packages/flake8",
   "files.exclude": {
      "**/.DS_Store": true,
      "**/.git": true,
      "**/.github": true,
      "**/.gitp*": true,
      "**/.hg": true,
      "**/.svn": true,
      "**/.vscode": true,
      "**/core.Microsoft*": true,
      "**/core.mongo*": true,
      "**/core.python*": true,
      "**/CVS": true
   },
   "files.autoSave": "off",
   "workbench.colorTheme": "Abyss",
   "terminal.integrated.gpuAcceleration": "canvas",
   "editor.defaultFormatter": "HookyQR.beautify",
   "cSpell.words": [
      "allauth",
      "dotenv",
      "MEDIAFILES",
      "socialaccount",
      "vapeshop",
      "vaportown"
   ],

}
